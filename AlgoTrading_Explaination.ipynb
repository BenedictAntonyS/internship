        "end_date = datetime.today()\n",
        "start_date = end_date - timedelta(days=180)\n",
        "\n",
        "# Download daily data for all stocks\n",
        "data = {}\n",
        "for symbol in stocks:\n",
        "    df = yf.download(symbol, start=start_date.strftime('%Y-%m-%d'), end=end_date.strftime('%Y-%m-%d'))\n",
